<?xml version="1.0" encoding="UTF-8"?>
<csw:GetRecords xmlns:csw="http://www.opengis.net/cat/csw/2.0.2" service="CSW" version="2.0.2" resultType="results" outputFormat="application/xml" startPosition="1" maxRecords="10">
  <csw:Query typeNames="csw:Record">
    <csw:ElementSetName>full</csw:ElementSetName>
    <csw:Constraint version="1.1.0">
      <ns1:Filter xmlns:ns1="http://www.opengis.net/ogc">
        <ns1:Intersects>
          <ns1:PropertyName>ows:BoundingBox</ns1:PropertyName>
          <ns2:Polygon xmlns:ns2="http://www.opengis.net/gml">
            <ns2:exterior>
              <ns2:LinearRing>
                <ns2:posList>47.0 -5.0 55.0 -5.0 55.0 20.0 47.0 20.0 47.0 -5.0</ns2:posList>
              </ns2:LinearRing>
            </ns2:exterior>
          </ns2:Polygon>
        </ns1:Intersects>
      </ns1:Filter>
    </csw:Constraint>
  </csw:Query>
</csw:GetRecords>
